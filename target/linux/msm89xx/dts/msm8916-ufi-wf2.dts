/dts-v1/;

#include "msm8916-ufi.dtsi"

/ {
	model = "Xinxun 4G Modem Stick (WF2)";
	compatible = "thwc,wf2", "qcom,msm8916";
};

&button_restart {
	gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>;
};

&button_default {
	pins = "gpio37";
	bias-pull-down;
};

&leds {
	sim-sel {
		label = "sim:sel";
		gpios = <&tlmm 97 GPIO_ACTIVE_HIGH>;
		default-state = "on";
	};

	sim-en {
		label = "sim:en";
		gpios = <&tlmm 108 GPIO_ACTIVE_HIGH>;
		default-state = "off";
	};

	sim-sel2 {
		label = "sim:sel2";
		gpios = <&tlmm 98 GPIO_ACTIVE_HIGH>;
		default-state = "off";
	};

	sim-en2 {
		label = "sim:en2";
		gpios = <&tlmm 38 GPIO_ACTIVE_HIGH>;
		default-state = "off";
	};

	led-r {
		gpios = <&tlmm 31 GPIO_ACTIVE_HIGH>;
	};

	led-g {
		gpios = <&tlmm 12 GPIO_ACTIVE_HIGH>;
	};

	led-b {
		gpios = <&tlmm 22 GPIO_ACTIVE_HIGH>;
	};
};

&gpio_leds_default {
	pins = "gpio12","gpio22","gpio31","gpio38","gpio97","gpio98", "gpio108";
};
